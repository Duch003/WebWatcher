<Application x:Class="WebWatcher.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WebWatcher.UI">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <local:Startup x:Key="Bootstrapper"/>
                </ResourceDictionary>
                <ResourceDictionary Source="./Styles/SliderStyles.xaml"/>
                <ResourceDictionary Source="./Styles/BorderStyles.xaml"/>
                <ResourceDictionary Source="./Styles/TextBoxStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
